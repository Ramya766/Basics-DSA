public class Main{
    public static void main(String[] args){
        int n=111;
        int orginal=n;
        int rev=0;
        while(n!=0){
            int dig=n%10;
            rev=rev*10+dig;
            n=n/10;
        }
        System.out.println(rev);
        if(rev==orginal){
            System.out.print("palindrome");
        }
        else{
            System.out.print("not a palindrome");
        }
    }
}
